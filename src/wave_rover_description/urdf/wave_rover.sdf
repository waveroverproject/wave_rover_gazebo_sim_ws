<?xml version="1.0" ?>
<sdf version="1.7">
  <model name="wave_rover">

    <!-- Base Link -->
    <link name="base_link">
      <gravity>true</gravity>
      <pose>0 0 0.3 0 0 0</pose>
      <inertial>
        <mass>2.0</mass>
        <inertia>
          <ixx>0.0083</ixx>
          <iyy>0.01</iyy>
          <izz>0.0125</izz>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
        </inertia>
      </inertial>
      <collision name="base_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.2 0.1</size>
          </box>
        </geometry>
      </collision>
      <visual name="base_visual">
        <geometry>
          <mesh>
            <uri>model://wave_rover_description/meshes/rover_body.stl</uri>
            <scale>0.001 0.001 0.001</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>0.6 0.6 0.6 1.0</ambient>
        </material>
      </visual>
    </link>

    <!-- Bracket and Camera Links -->
    <link name="bracket_stand_link">
      <pose>0 0.0042 0.07 0 0 0</pose>
      <inertial>
        <mass>0.001</mass>
        <inertia>
          <ixx>0.000001</ixx>
          <iyy>0.000001</iyy>
          <izz>0.000001</izz>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
        </inertia>
      </inertial>
      <visual name="bracket_stand_visual">
        <geometry>
          <box>
            <size>0.045 0.11 0.04</size>
          </box>
        </geometry>
        <material>
          <ambient>0.1 0.1 0.1 1.0</ambient>
        </material>
      </visual>
    </link>

    <link name="camera_bracket_link">
      <pose>0 -0.0358 0.09 0 0 0</pose>
      <inertial>
        <mass>0.001</mass>
        <inertia>
          <ixx>0.000001</ixx>
          <iyy>0.000001</iyy>
          <izz>0.000001</izz>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
        </inertia>
      </inertial>
      <visual name="camera_bracket_visual">
        <geometry>
          <mesh>
            <uri>model://wave_rover_description/meshes/realsense_bracket.stl</uri>
            <scale>0.001 0.001 0.001</scale>
          </mesh>
        </geometry>
      </visual>
    </link>

    <link name="camera_link">
      <pose>0 -0.0658 0.11 0.349 0 0</pose>
      <inertial>
        <mass>0.001</mass>
        <inertia>
          <ixx>0.000001</ixx>
          <iyy>0.000001</iyy>
          <izz>0.000001</izz>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
        </inertia>
      </inertial>
      <visual name="camera_visual">
        <geometry>
          <mesh>
            <uri>model://wave_rover_description/meshes/d435.stl</uri>
            <scale>0.01 0.01 0.01</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>0.1 0.1 1.0 1.0</ambient>
        </material>
      </visual>
      <sensor name="depth_camera" type="depth">
        <pose>0 0 0 0.349 0 -1.5708</pose>
        <update_rate>30.0</update_rate>
        <camera>
          <horizontal_fov>1.3962634</horizontal_fov>
          <image>
            <width>640</width>
            <height>480</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.1</near>
            <far>5.0</far>
          </clip>
        </camera>
        <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
          <ros>
            <namespace>/camera</namespace>
          </ros>
          <camera_name>camera</camera_name>
          <image_topic_name>image_raw</image_topic_name>
          <depth_image_topic_name>depth/image_raw</depth_image_topic_name>
          <info_topic_name>camera_info</info_topic_name>
          <point_cloud_topic_name>depth/points</point_cloud_topic_name>
          <frame_name>camera_link_optical</frame_name>
        </plugin>
      </sensor>
    </link>

    <link name="camera_link_optical">
      <pose>0 0 0 -1.5708 0 -1.5708</pose>
      <inertial>
        <mass>0.001</mass>
        <inertia>
          <ixx>0.000001</ixx>
          <iyy>0.000001</iyy>
          <izz>0.000001</izz>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
        </inertia>
      </inertial>
    </link>

    <!-- Fixed Joints -->
    <joint name="bracket_stand_mount" type="fixed">
      <parent>base_link</parent>
      <child>bracket_stand_link</child>
    </joint>
    
    <joint name="bracket_mount" type="fixed">
      <parent>bracket_stand_link</parent>
      <child>camera_bracket_link</child>
    </joint>
    
    <joint name="camera_mount" type="fixed">
      <parent>camera_bracket_link</parent>
      <child>camera_link</child>
    </joint>
    
    <joint name="camera_optical_joint" type="fixed">
      <parent>camera_link</parent>
      <child>camera_link_optical</child>
    </joint>

    <!-- Wheel Links -->
    <link name="front_left_wheel">
      <pose>-0.3 0.18 0.05 0 0 0</pose>
      <inertial>
	  <mass>0.2</mass>
	  <inertia>
	    <ixx>0.0001</ixx>
	    <iyy>0.0001</iyy>
	    <izz>0.0001</izz>
	    <ixy>0.0</ixy>
	    <ixz>0.0</ixz>
	    <iyz>0.0</iyz>
	  </inertia>
	</inertial>
      <collision name="front_left_collision">
        <geometry>
          <cylinder><radius>0.05</radius><length>0.02</length></cylinder>
        </geometry>
      </collision>
      <visual name="front_left_visual">
        <geometry>
          <mesh><uri>model://wave_rover_description/meshes/front_left_wheel.stl</uri><scale>0.001 0.001 0.001</scale></mesh>
        </geometry>
      </visual>
    </link>
    
    <link name="front_right_wheel">
      <pose>-0.3 -0.18 0.05 0 0 0</pose>
      <inertial>
	  <mass>0.2</mass>
	  <inertia>
	    <ixx>0.0001</ixx>
	    <iyy>0.0001</iyy>
	    <izz>0.0001</izz>
	    <ixy>0.0</ixy>
	    <ixz>0.0</ixz>
	    <iyz>0.0</iyz>
	  </inertia>
	</inertial>
      <collision name="front_right_collision">
        <geometry>
          <cylinder><radius>0.05</radius><length>0.02</length></cylinder>
        </geometry>
      </collision>
      <visual name="front_right_visual">
        <geometry>
          <mesh><uri>model://wave_rover_description/meshes/front_right_wheel.stl</uri><scale>0.001 0.001 0.001</scale></mesh>
        </geometry>
      </visual>
    </link>
    
    <link name="back_left_wheel">
      <pose>0.3 0.18 0.05 0 0 0</pose>
      <inertial>
	  <mass>0.2</mass>
	  <inertia>
	    <ixx>0.0001</ixx>
	    <iyy>0.0001</iyy>
	    <izz>0.0001</izz>
	    <ixy>0.0</ixy>
	    <ixz>0.0</ixz>
	    <iyz>0.0</iyz>
	  </inertia>
	</inertial>
      <collision name="back_left_collision">
        <geometry>
          <cylinder><radius>0.05</radius><length>0.02</length></cylinder>
        </geometry>
      </collision>
      <visual name="back_left_visual">
        <geometry>
          <mesh><uri>model://wave_rover_description/meshes/back_left_wheel.stl</uri><scale>0.001 0.001 0.001</scale></mesh>
        </geometry>
      </visual>
    </link>
    
    <link name="back_right_wheel">
      <pose>0.3 -0.18 0.05 0 0 0</pose>
      <inertial>
	  <mass>0.2</mass>
	  <inertia>
	    <ixx>0.0001</ixx>
	    <iyy>0.0001</iyy>
	    <izz>0.0001</izz>
	    <ixy>0.0</ixy>
	    <ixz>0.0</ixz>
	    <iyz>0.0</iyz>
	  </inertia>
	</inertial>
      <collision name="back_right_collision">
        <geometry>
          <cylinder><radius>0.05</radius><length>0.02</length></cylinder>
        </geometry>
      </collision>
      <visual name="back_right_visual">
        <geometry>
          <mesh><uri>model://wave_rover_description/meshes/back_right_wheel.stl</uri><scale>0.001 0.001 0.001</scale></mesh>
        </geometry>
      </visual>
    </link>

    <!-- Wheel Joints with Damping -->
    <joint name="front_left_wheel_joint" type="revolute">
      <parent>base_link</parent>
      <child>front_left_wheel</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit><lower>-1000</lower><upper>1000</upper><effort>10</effort><velocity>10</velocity></limit>
        <dynamics><damping>0.5</damping></dynamics>
      </axis>
    </joint>
    <joint name="front_right_wheel_joint" type="revolute">
      <parent>base_link</parent>
      <child>front_right_wheel</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit><lower>-1000</lower><upper>1000</upper><effort>10</effort><velocity>10</velocity></limit>
        <dynamics><damping>0.5</damping></dynamics>
      </axis>
    </joint>
    <joint name="back_left_wheel_joint" type="revolute">
      <parent>base_link</parent>
      <child>back_left_wheel</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit><lower>-1000</lower><upper>1000</upper><effort>10</effort><velocity>10</velocity></limit>
        <dynamics><damping>0.5</damping></dynamics>
      </axis>
    </joint>
    <joint name="back_right_wheel_joint" type="revolute">
      <parent>base_link</parent>
      <child>back_right_wheel</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit><lower>-1000</lower><upper>1000</upper><effort>10</effort><velocity>10</velocity></limit>
        <dynamics><damping>0.5</damping></dynamics>
      </axis>
    </joint>

    <!-- Diff Drive Plugin -->
    <gazebo>
	    <plugin name="diff_drive_controller" filename="libgazebo_ros_diff_drive.so">
	      <ros>
		<namespace>/wave_rover</namespace>
	      </ros>
	      <num_wheel_pairs>2</num_wheel_pairs>
	      <left_joint>front_left_wheel_joint</left_joint>
	      <left_joint>back_left_wheel_joint</left_joint>
	      <right_joint>front_right_wheel_joint</right_joint>
	      <right_joint>back_right_wheel_joint</right_joint>
	      <wheel_separation>0.132</wheel_separation>
	      <wheel_diameter>0.08</wheel_diameter>
	      <max_wheel_torque>20.0</max_wheel_torque>
	      <wheelAcceleration>1.0</wheelAcceleration>
	      <max_wheel_acceleration>10.0</max_wheel_acceleration>
	      <command_topic>cmd_vel</command_topic>
	      <odometry_topic>odom</odometry_topic>
	      <odometrySource>1</odometrySource>
	      <odometry_frame>odom</odometry_frame>
	      <robot_base_frame>base_link</robot_base_frame>
	      <publish_odom>true</publish_odom>
	      <publish_odom_tf>true</publish_odom_tf>
	      <publish_wheel_tf>false</publish_wheel_tf>
	    </plugin>	
    </gazebo>

    <static>false</static>
  </model>
</sdf>

